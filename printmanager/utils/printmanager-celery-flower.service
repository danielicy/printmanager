[Unit]
Description=PrintManager Celery Worker (Watcher)
After=network.target

[Service]
User=vboxuser
Group=vboxuser
WorkingDirectory=/home/vboxuser/printmanager
Environment="PATH=/home/vboxuser/printmanager/venv/bin"
ExecStart=/home/vboxuser/printmanager/venv/bin/celery -A printmanager flower --port=5555
Restart=always

[Install]
WantedBy=multi-user.target


